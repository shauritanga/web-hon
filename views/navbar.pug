nav.navbar.navbar-expand-lg.navbar-dark.bg-dark
  a.navbar-brand(href='/') Instructors
  button.navbar-toggler(type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation")
    span.navbar-toggler-icon
  div.collapse.navbar-collapse#navbarSupportedContent
    ul.navbar-nav.mr-auto
      if currentUser
        li.nav-item
        a.nav-link(href ='material') Materials
      li.nav-item
        a.nav-link(href ='about') About Us
      li.nav-item
        a.nav-link(href ='contact') Contact Us
      if !currentUser
        li.nav-item
          a.nav-link(href ='register') Sign Up
      li.nav-item
        a.nav-link(href ='profile') Profile
    if currentUser
      div
        a.btn.btn-info.pull-md-right(href = 'logout')
          i.fa.fa-user 
          span  Logout
    else
      a.btn.btn-info.pull-md-right(href = 'login') Login
